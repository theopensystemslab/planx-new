{
  "name": "api.planx.uk",
  "license": "MPL-2.0",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "type": "module",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=22.14",
    "pnpm": "10.10.0"
  },
  "dependencies": {
    "@airbrake/node": "^2.1.8",
    "@aws-sdk/client-s3": "^3.696.0",
    "@aws-sdk/s3-request-presigner": "^3.701.0",
