    "prepare": "cd .. && husky install editor.planx.uk/.husky",
    "preinstall": "pnpm install ./shared/core*",
    "reset:core": "git checkout HEAD pnpm-lock.yaml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.([jt]sx?)$": "esbuild-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown)/"
    ],
    "resetMocks": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
