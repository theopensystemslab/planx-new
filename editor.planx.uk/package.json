  "packageManager": "pnpm@8.6.6",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:silent": "vitest --silent",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "mkdir -p build && storybook build --quiet --output-dir ./build/storybook",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' && prettier -c ./src",
    "lint:error": "eslint --quiet 'src/**/*.{js,jsx,ts,tsx}' && prettier -c ./src",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' && prettier -w ./src",
    "check": "tsc --noEmit && pnpm lint",
    "prepare": "cd .. && husky install editor.planx.uk/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown)/"
    ],
    "resetMocks": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "scripts/tsc-lint.sh"
    ]
  },
  "pnpm": {
    "overrides": {
      "postcss@<8.4.31": ">=8.4.31",
      "nth-check@<2.0.1": ">=2.0.1",
      "braces@<3.0.3": ">=3.0.3",
      "micromatch@<4.0.8": ">=4.0.8"
    }
  }
