  "scripts": {
    "start": "craco start",
    "build": "CI=false && craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "classic:start": "react-app-rewired start",
    "classic:build": "react-scripts build",
    "classic:test": "react-scripts test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "mkdir -p build && storybook build --quiet --output-dir ./build/storybook",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' && prettier -c ./src",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' && prettier -w ./src",
    "check": "tsc --noEmit && pnpm lint",
    "prepare": "cd .. && husky install editor.planx.uk/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.([jt]sx?)$": "esbuild-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown)/"
    ],
    "resetMocks": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "scripts/tsc-lint.sh"
    ]
  },
  "pnpm": {
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "postcss@<8.4.31": ">=8.4.31",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "@adobe/css-tools@<4.3.2": ">=4.3.2"
    }
  }
