  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "classic:start": "react-app-rewired start",
    "classic:build": "react-scripts build",
    "classic:test": "react-scripts test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "mkdir -p build && build-storybook --quiet --output-dir ./build/storybook",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "cd .. && husky install editor.planx.uk/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.([jt]sx?)$": "esbuild-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown)/"
    ],
    "resetMocks": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "scripts/tsc-lint.sh"
    ]
  }
